<Window x:Class="GymSysyemWpf.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindowView" Height="700" Width="1300"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        Background="Transparent"
        AllowsTransparency="True"
        PreviewKeyDown="Window_PreviewKeyDown"
        >
    <!--MouseDown="Window_MouseDown"-->
    <!--MouseDown="Window_MouseDown"-->

    <Window.Resources>
        <Style x:Key="CenterGridHeaderStyle" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
        </Style>

        <ControlTemplate x:Key="NoMouseOverButtonTemplate" 
                 TargetType="Button">
            <Border Background="{TemplateBinding Background}"
          BorderBrush="{TemplateBinding BorderBrush}"
          BorderThickness="{TemplateBinding BorderThickness}">
                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
            </Border>

            <!-- Add only required visual state triggers -->
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled"
             Value="False">
                    <Setter Property="Background"
              Value="{x:Static SystemColors.ControlLightBrush}" />
                    <Setter Property="Foreground"
              Value="{x:Static SystemColors.GrayTextBrush}" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>

        <ControlTemplate x:Key="NoMouseOverRadioButton" 
                 TargetType="RadioButton">
            <Border Background="{TemplateBinding Background}"
          BorderBrush="{TemplateBinding BorderBrush}"
          BorderThickness="{TemplateBinding BorderThickness}">
                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
            </Border>

            <!-- Add only required visual state triggers -->
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled"
             Value="False">
                    <Setter Property="Background"
              Value="{x:Static SystemColors.ControlLightBrush}" />
                    <Setter Property="Foreground"
              Value="{x:Static SystemColors.GrayTextBrush}" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
    </Window.Resources>

    <Border CornerRadius="10"
            FlowDirection="RightToLeft"
            BorderThickness="1">
        <Border.BorderBrush>
            <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                <GradientStop Color="{StaticResource winBorderColor1}" Offset="0"/>
                <GradientStop Color="{StaticResource winBorderColor2}" Offset="0.5"/>
                <GradientStop Color="{StaticResource winBorderColor3}" Offset="1"/>
            </LinearGradientBrush>
        </Border.BorderBrush>

        <Grid>
            <Grid.ColumnDefinitions>
                <!--Navigation menu-->
                <ColumnDefinition Width="250"/>
                <!--Content section-->
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!--Right Section-->
            <Border CornerRadius="10,0,0,10"
                    Grid.Column="0">
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,0.7">
                        <GradientStop Color="{StaticResource secondaryBackColor1}" Offset="0"/>
                        <GradientStop Color="{StaticResource secondaryBackColor2}" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <StackPanel Margin="10">

                    <!--Logo-->
                    <StackPanel Orientation="Horizontal"
                                Height="70"
                                Margin="15,10,0,15">

                        <Image  Height="50" Width="50" Source="/gymLog.png"/>
                        <TextBlock Text="Gym"
                                   Foreground="{StaticResource titleColor2}"
                                   FontSize="20"
                                   FontFamily="Montserrat"
                                   FontWeight="Medium"
                                   VerticalAlignment="Center"
                                   Margin="10,0,0,0"/>
                    </StackPanel>

                    <!--Right Menu Buttons-->

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="traniee"
                                 Click="traniee_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/athlete-svgrepo-com.png" ></Image>
                            <TextBlock Text="المتدربين" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>

                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="attendance"
                                 Click="attendance_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/athlete-svgrepo-com.png" ></Image>
                            <TextBlock Text="الحضور" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>

                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="Subscription"
                                 Click="Subscripe_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/bottle-water-svgrepo-com.png" ></Image>
                            <TextBlock Text="الاشتراكات" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="protien"
                                 Click="protien_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/bottle-water-svgrepo-com.png" ></Image>
                            <TextBlock Text="إضافة منتج" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="BUyProtein"
                                 Click="BUYProtein_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/buyer-account-svgrepo-com.png" ></Image>
                            <TextBlock Text=" المبيعات" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>
                    
                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="bill"
                                 Click="bill_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/bottle-water-svgrepo-com.png" ></Image>
                            <TextBlock Text="فواتير" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="deleted"
                                 Click="delete_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/athlete-svgrepo-com.png" ></Image>
                            <TextBlock Text="المحذوفات" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>

                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 
                                 Margin="20,0"
                                 Name="financial"
                                 Click="financial_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/money-send-svgrepo-com.png" ></Image>
                            <TextBlock Text="تقرير مالي" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Name="changePass"
                                 Click="changePass_Click">
                        <!--Command="{Binding ShowHomeViewCommand}">-->
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/images/password.png" ></Image>
                            <TextBlock Text="تغير الارقام السريه" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>


                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,50"
                                 Name="logoutAdmin"
                                 Click="logout_Click"
                                 >
                        <!--Command="{Binding ShowHomeViewCommand}">-->
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="تسجيل الخروج" FontSize="20" Style="{StaticResource menuButtonText}"/>
                            <Icon:PackIconMaterial Kind="Logout" Margin="7" Foreground="#e5ac03" Width="20" Height="20"></Icon:PackIconMaterial>
                        </StackPanel>
                    </RadioButton>
                    
                    
                </StackPanel>
            </Border>




            <!--Left Section-->
            <Border CornerRadius="0,10,10,0"
                    Grid.Column="1">
                <Border.Background>
                    <LinearGradientBrush StartPoint="1,1" EndPoint="0,0">
                        <GradientStop Color="White" Offset="0"/>
                        <GradientStop Color="WhiteSmoke" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <Grid>
                    <Grid.RowDefinitions>
                        <!--nav control-->
                        <RowDefinition Height="35"/>

                        <!--header-->
                        <RowDefinition  Height="40"/>

                        <RowDefinition  Height="40"/>

                        <!-- grid view-->
                        <RowDefinition Height="500" />

                        <RowDefinition Height="80" />
                    </Grid.RowDefinitions>

                    <!--control bar-->
                    <StackPanel Grid.Row="0"
                                x:Name="pnlControlBar"
                                Orientation="Horizontal"
                                FlowDirection="LeftToRight"
                                Background="Transparent"
                                Margin="0,10,15,0" >



                        <Button x:Name="btnClose"                          
                            BorderThickness="0"
                            Content="X"
                            Foreground="Black"
                            FontSize="16"
                            FontFamily="Montserrat"
                            Cursor="Hand"
                            Margin="2"
                            Click="btnClose_Click">
                            <Button.Style>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#DA34AE"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#C62DAE"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border Width="18" Height="18"
                                        CornerRadius="9"
                                        Background="{TemplateBinding Background}">
                                        <ContentPresenter VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                        <Button x:Name="btnMaximize"                           
                            BorderThickness="0"
                            Foreground="White"
                            FontSize="18"
                            FontFamily="Montserrat"
                            Cursor="Hand"
                            Margin="2"
                            Click="btnMaximize_Click">
                            <Image  Width="12" Source="/max.png"></Image>
                            <Button.Style>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#28AEED"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#278BEF"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border Width="18" Height="18"
                                        CornerRadius="9"
                                        Background="{TemplateBinding Background}">
                                        <ContentPresenter VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                        <Button x:Name="btnMinimize"                           
                            BorderThickness="0"
                            Content="-"
                            Foreground="black"
                            FontSize="20"
                            FontFamily="Montserrat"
                            Cursor="Hand"
                            Margin="2"
                            Click="btnMinimize_Click">
                            <Button.Style>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#28AEED"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#278BEF"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border Width="18" Height="18"
                                        CornerRadius="9"
                                        Background="{TemplateBinding Background}">
                                        <ContentPresenter VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                    </StackPanel>

                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="67*"></ColumnDefinition>
                            <ColumnDefinition Width="195*"/>
                            <ColumnDefinition Width="240*"></ColumnDefinition>
                            <ColumnDefinition Width="22*"/>
                        </Grid.ColumnDefinitions>

                        <StackPanel Grid.Column="0"
                                    Orientation="Horizontal"
                                    VerticalAlignment="Center" Grid.ColumnSpan="2">

                            <TextBlock  Text="مرحباً بك"
                                        Grid.Column="1"
                                        Name="headerText"
                                       Foreground="Black"
                                       Margin="10"
                                       FontSize="20"
                                       FontFamily="Montserrat"
                                       FontWeight="DemiBold"
                                       VerticalAlignment="Center">
                            </TextBlock>
                            <TextBox x:Name="sellingProduct" 
                                       Width="300"
                                       Visibility="Hidden"
                                       FontSize="20"
                                       Margin="100,0,0,0"
                                       Background="AliceBlue"></TextBox>
                            <!--GotFocus="sellingProduct_GotFocus"-->
                        </StackPanel>

                    </Grid>

                    <StackPanel Grid.Row="2" Orientation="Horizontal" >

                        <!--Subscription Buttons-->
                        <StackPanel HorizontalAlignment="Right" 
                                    Orientation="Horizontal"
                                    Margin="20 0 0 0"
                                    >
                            <Button x:Name="btnSpcail"
                                    BorderThickness="0"
                                    Visibility="Hidden"
                                    Content="حصريا"
                                    Foreground="White"
                                    FontSize="15"
                                    FontFamily="Montserrat"
                                    Cursor="Hand"
                                    Click="btnLogin_Click">
                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Setter Property="Background" Value="#462AD8"/>
                                        <Style.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter Property="Background" Value="#28AEED"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                                <Button.Template>
                                    <ControlTemplate TargetType="Button">
                                        <Border Width="100" 
                                                Height="40"
                                                CornerRadius="20"
                                                Background="{TemplateBinding Background}">
                                            <ContentPresenter VerticalAlignment="Center"
                                      HorizontalAlignment="Center"/>
                                        </Border>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>

                            <Button x:Name="btnSub"
                                    BorderThickness="0"
                                    Visibility="Hidden"
                                    Content="الاشتراكات"
                                    Foreground="White"
                                    FontSize="15"
                                    FontFamily="Montserrat"
                                    Cursor="Hand"
                                    Click="btnSub_Click">
                                <Button.Style>
                                    <Style TargetType="Button">
                                        <Setter Property="Background" Value="#462AD8"/>
                                        <Style.Triggers>
                                            <Trigger Property="IsMouseOver" Value="True">
                                                <Setter Property="Background" Value="#28AEED"/>
                                            </Trigger>
                                        </Style.Triggers>
                                    </Style>
                                </Button.Style>
                                <Button.Template>
                                    <ControlTemplate TargetType="Button">
                                        <Border Width="100" 
                                                Height="40"
                                                CornerRadius="20"
                                                Background="{TemplateBinding Background}">
                                            <ContentPresenter VerticalAlignment="Center"
                                      HorizontalAlignment="Center"/>
                                        </Border>
                                    </ControlTemplate>
                                </Button.Template>
                            </Button>
                        </StackPanel>

                        <!--Search bar-->
                        <Grid HorizontalAlignment="Center">

                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="570"/>
                            </Grid.ColumnDefinitions>

                            <Grid>
                                <TextBox  Name="Search" 
                                          Visibility="Hidden"
                                          Foreground="Black"
                                          Background= "Beige"
                                          FontSize="20"
                                          Height="30"
                                          Padding="10,0,0,0"
                                          FontFamily="Montserrat"
                                          FontWeight="DemiBold"
                                          TextChanged="Search_TextChanged"
                                          
                                          >
                                </TextBox>

                                <Border Visibility="Hidden" Name="SerchAdd" >
                                    <Canvas  Height="40" Name="add" >

                                        <Icon:PackIconMaterial Kind="SearchWeb"
                                                           Name="searsh"
                                                           Visibility="Hidden"
                                                           Foreground="Black"
                                                           Canvas.Right="10"
                                                           Width="22"
                                                           Height="22"
                                                           Canvas.Bottom="10"
                                                           MouseDown="searsh_MouseDown" 
                                                               
                                                           
                                                               >

                                        </Icon:PackIconMaterial>
                                    </Canvas>
                                </Border>
                            </Grid>

                        </Grid>



                    </StackPanel>



                    <!-- Trainee grid-->
                    <Border Grid.Row="3"
                            Background="#200F53"
                            CornerRadius="10" 
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="traineeGrid"
                   >

                        <DataGrid Name="dataGrideTrainee" LoadingRow="dataGrideTrainee_LoadingRow" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*"
                                  Background="Wheat"
                                  BorderThickness="0"
                                  GridLinesVisibility="None"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
                                  TextBlock.FontSize="12" 
                                  >

                            <DataGrid.Columns >

                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="#" Binding="{Binding order}" IsReadOnly="True" Width="0.2*" />
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="الأسم" Binding="{Binding Name}" IsReadOnly="True" Width="0.5*" />
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="رقم التلفون" Binding="{Binding Phone}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="ت البدايه" Binding="{Binding startdate, StringFormat={}{0:dd/MM/yyyy}}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="ت النهاية" Binding="{Binding EndDate, StringFormat={}{0:dd/MM/yyyy}}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="نوع الأشتراك" Binding="{Binding SubsName}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="عدد السيشنز" Binding="{Binding NumberOfAttendance}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="السعر" Binding="{Binding Price}" IsReadOnly="True" Width="0.5*"/>

                                <!--Operations-->
                                <DataGridTemplateColumn
                                    Header="تعديل" IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel 
                                                
                                                Orientation="Horizontal">

                                                <Border Grid.Row="3" Visibility="Visible" Name="Tupdate"  >
                                                    <Canvas  
                                                        Height="25" HorizontalAlignment="Left"  Name="U" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}"  Style="{StaticResource buttonIconHover}" Background="Transparent" BorderThickness="0" Click="TrEdit_Click">


                                                            <Icon:PackIconMaterial Kind="PencilOutline"
                                                                               
                                                                                Foreground="Wheat" 
                                                                                FontSize="8"
                                                                                Width="15"
                                                                                Height="15"
                                                                                   Margin="4,2">


                                                            </Icon:PackIconMaterial>
                                                        </Button>

                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn Header="مسح" IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="5" Visibility="Visible" Name="CDelete">
                                                    <Canvas  Height="25"  Name="D" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}" Background="Transparent" BorderThickness="0" Click="TrDelete_Click">

                                                            <Icon:PackIconMaterial Kind="DeleteOutline" 
                                                                             
                                                               Foreground="Wheat" 
                                                               FontSize="8"
                                                               Width="15"
                                                               Height="15"
                                                               Margin="4,2"
                                                               HorizontalAlignment="Left"
                                                               >
                                                            </Icon:PackIconMaterial>
                                                        </Button>
                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>



                            </DataGrid.Columns>


                        </DataGrid>



                    </Border>

                    <!-- Attendance grid-->
                    <Border Grid.Row="3"
                            Background="#200F53"
                            CornerRadius="10" 
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="attendanceGrid"
                   >

                        <DataGrid Name="dataGrideAttendance" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*"
                                  Background="Wheat"
                                  BorderThickness="0"
                                  GridLinesVisibility="None"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
                                  TextBlock.FontSize="12" 
                                  >

                            <DataGrid.Columns >
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="#" Binding="{Binding order}" IsReadOnly="True" Width="0.2*" />
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="الأسم" Binding="{Binding Name}" IsReadOnly="True" Width="0.5*" />
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="رقم التلفون" Binding="{Binding Phone}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header=" ت البداية" Binding="{Binding startdate, StringFormat={}{0:MM/dd/yyyy}}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="ت النهاية" Binding="{Binding EndDate, StringFormat={}{0:MM/dd/yyyy}}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="نوع الأشتراك" Binding="{Binding SubsName}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="عدد السيشنز" Binding="{Binding NumberOfAttendance}" IsReadOnly="True" Width="0.5*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="السعر" Binding="{Binding Price}" IsReadOnly="True" Width="0.5*"/>

                            </DataGrid.Columns>


                        </DataGrid>



                    </Border>

                    <!-- Subscripe grid-->
                    <Border Grid.Row="3"
                            Background="#200F53"
                            CornerRadius="10" 
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="SubscripeGrid"
                   >

                        <DataGrid Name="dataGrideSubscripe" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*"
                                  Background="Wheat"
                                  BorderThickness="0"
                                  GridLinesVisibility="None"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
                                  TextBlock.FontSize="12" 
                                  >

                            <DataGrid.Columns >

                                <!--<DataGridTemplateColumn Header="طباعة الكود" IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="3" Visibility="Visible" Name="Tupdate"  >
                                                    <Canvas  Height="25" HorizontalAlignment="Left"  Name="U" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}"  Style="{StaticResource buttonIconHover}" Background="Transparent" BorderThickness="0" Click="TrEdit_Click">


                                                            <Icon:PackIconMaterial Kind="Qrcode"
                                                                               
                                                                                Foreground="Wheat" 
                                                                                FontSize="8"
                                                                                Width="15"
                                                                                Height="15"
                                                                                   Margin="4,2">


                                                            </Icon:PackIconMaterial>
                                                        </Button>

                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
