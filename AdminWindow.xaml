<Window x:Class="GymSysyemWpf.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks"
        xmlns:local="clr-namespace:GymSysyemWpf"
        xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
        mc:Ignorable="d"
        Title="MainWindowView" Height="700" Width="1300"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        Background="Transparent"
        AllowsTransparency="True"
        
        >
        <!--MouseDown="Window_MouseDown"-->
    <!--MouseDown="Window_MouseDown"-->

    <Window.Resources>
        <Style x:Key="CenterGridHeaderStyle" TargetType="DataGridColumnHeader">
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
        </Style>

        <ControlTemplate x:Key="NoMouseOverButtonTemplate" 
                 TargetType="Button">
            <Border Background="{TemplateBinding Background}"
          BorderBrush="{TemplateBinding BorderBrush}"
          BorderThickness="{TemplateBinding BorderThickness}">
                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
            </Border>

            <!-- Add only required visual state triggers -->
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled"
             Value="False">
                    <Setter Property="Background"
              Value="{x:Static SystemColors.ControlLightBrush}" />
                    <Setter Property="Foreground"
              Value="{x:Static SystemColors.GrayTextBrush}" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>

        <ControlTemplate x:Key="NoMouseOverRadioButton" 
                 TargetType="RadioButton">
            <Border Background="{TemplateBinding Background}"
          BorderBrush="{TemplateBinding BorderBrush}"
          BorderThickness="{TemplateBinding BorderThickness}">
                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                      VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
            </Border>

            <!-- Add only required visual state triggers -->
            <ControlTemplate.Triggers>
                <Trigger Property="IsEnabled"
             Value="False">
                    <Setter Property="Background"
              Value="{x:Static SystemColors.ControlLightBrush}" />
                    <Setter Property="Foreground"
              Value="{x:Static SystemColors.GrayTextBrush}" />
                </Trigger>
            </ControlTemplate.Triggers>
        </ControlTemplate>
    </Window.Resources>
    
    <Border CornerRadius="10"                    
            BorderThickness="1">
        <Border.BorderBrush>
            <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
                <GradientStop Color="{StaticResource winBorderColor1}" Offset="0"/>
                <GradientStop Color="{StaticResource winBorderColor2}" Offset="0.5"/>
                <GradientStop Color="{StaticResource winBorderColor3}" Offset="1"/>
            </LinearGradientBrush>
        </Border.BorderBrush>

        <Grid>
            <Grid.ColumnDefinitions>
                <!--Navigation menu-->
                <ColumnDefinition Width="250"/>
                <!--Content section-->
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <Border CornerRadius="10,0,0,10"
                    
                    Grid.Column="0">
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="1,0.7">
                        <GradientStop Color="{StaticResource secondaryBackColor1}" Offset="0"/>
                        <GradientStop Color="{StaticResource secondaryBackColor2}" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <StackPanel Margin="20">

                    <!--Logo-->
                    <StackPanel Orientation="Horizontal"
                                Height="70"
                                Margin="15,25,0,40">

                        <Image  Height="50" Width="50" Source="/gymLog.png"/>
                        <TextBlock Text="Heroes Gym"
                                   Foreground="{StaticResource titleColor2}"
                                   FontSize="20"
                                   FontFamily="Montserrat"
                                   FontWeight="Medium"
                                   VerticalAlignment="Center"
                                   Margin="10,0,0,0"/>
                    </StackPanel>

                    <!--Menu Buttons-->
                    <RadioButton x:Name="receptionist"
                        Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 
                                 Click="receptionist_Click">

                        <StackPanel Orientation="Horizontal">
                            <!--e Ico <fa:IconImagn="Home" Style="{StaticResource menuButtonIcon}"/>-->
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/reception-2-svgrepo-com.png"></Image>
                            <TextBlock Text="Reciptionist" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="traniee"
                                 Click="traniee_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/athlete-svgrepo-com.png" ></Image>
                            <TextBlock Text="Trainee" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>

                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="coach"
                                 Click="coach_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/muscle-svgrepo-com.png" ></Image>
                            <TextBlock Text="Coach" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="protien"
                                 Click="protien_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/bottle-water-svgrepo-com.png" ></Image>
                            <TextBlock Text="Protien Products" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>


                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="machiene"
                                 Click="machiene_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/gym-dumbbell-svgrepo-com.png" ></Image>
                            <TextBlock Text="Machines" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="Vendors"
                                 Click="Vendor_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/admin-svgrepo-com.png" ></Image>
                            <TextBlock Text="Vendors" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>


                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Command="{Binding ShowHomeViewCommand}"
                                 Name="BUyProtein"
                                 Click="BUYProtein_Click">
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/buyer-account-svgrepo-com.png" ></Image>
                            <TextBlock Text="Selling Protein" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>

                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,0"
                                 Name="financial"
                                 Click="financial_Click">
                        <!--Command="{Binding ShowHomeViewCommand}">-->
                        <StackPanel Orientation="Horizontal">
                            <Image  Margin="0 0 5 0" Width="28" Height="28" Source="/money-send-svgrepo-com.png" ></Image>
                            <TextBlock Text="FINANCIAL REPORT" Style="{StaticResource menuButtonText}"/>
                        </StackPanel>
                    </RadioButton>


                    <RadioButton Style="{StaticResource menuButton}"
                                 Tag="{StaticResource color1}"
                                 IsChecked="True"
                                 Margin="20,50"
                                 Name="logoutAdmin"
                                 Click="logout_Click"
                                 >
                        <!--Command="{Binding ShowHomeViewCommand}">-->
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Text="Log out" FontSize="20" Style="{StaticResource menuButtonText}"/>
                            <Icon:PackIconMaterial Kind="Logout" Margin="7" Foreground="#e5ac03" Width="20" Height="20"></Icon:PackIconMaterial>
                        </StackPanel>
                    </RadioButton>
                </StackPanel>
            </Border>

            <!--Main Section-->
            <Border CornerRadius="0,10,10,0"
                    Grid.Column="1">
                <Border.Background>
                    <LinearGradientBrush StartPoint="1,1" EndPoint="0,0">
                        <GradientStop Color="White" Offset="0"/>
                        <GradientStop Color="WhiteSmoke" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>

                <Grid>
                    <Grid.RowDefinitions>
                        <!--nav control-->
                        <RowDefinition Height="35"/>

                        <!--header-->
                        <RowDefinition  Height="40"/>

                        <!-- grid view-->
                        <RowDefinition Height="540" />

                        <RowDefinition Height="60" />
                    </Grid.RowDefinitions>

                    <!--control bar-->
                    <StackPanel x:Name="pnlControlBar"
                                Grid.Row="0"
                                Orientation="Horizontal"
                                FlowDirection="RightToLeft"
                                Background="Transparent"
                                Margin="0,10,15,0" >



                        <Button x:Name="btnClose"                          
                            BorderThickness="0"
                            Content="X"
                            Foreground="Black"
                            FontSize="16"
                            FontFamily="Montserrat"
                            Cursor="Hand"
                            Margin="2"
                            Click="btnClose_Click">
                            <Button.Style>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#DA34AE"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#C62DAE"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border Width="18" Height="18"
                                        CornerRadius="9"
                                        Background="{TemplateBinding Background}">
                                        <ContentPresenter VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                        <Button x:Name="btnMaximize"                           
                            BorderThickness="0"
                            Foreground="White"
                            FontSize="18"
                            FontFamily="Montserrat"
                            Cursor="Hand"
                            Margin="2"
                            Click="btnMaximize_Click">
                            <Image  Width="12" Source="/max.png"></Image>
                            <Button.Style>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#28AEED"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#278BEF"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border Width="18" Height="18"
                                        CornerRadius="9"
                                        Background="{TemplateBinding Background}">
                                        <ContentPresenter VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                        <Button x:Name="btnMinimize"                           
                            BorderThickness="0"
                            Content="-"
                            Foreground="black"
                            FontSize="20"
                            FontFamily="Montserrat"
                            Cursor="Hand"
                            Margin="2"
                            Click="btnMinimize_Click">
                            <Button.Style>
                                <Style TargetType="Button">
                                    <Setter Property="Background" Value="#28AEED"/>
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="#278BEF"/>
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Button.Style>
                            <Button.Template>
                                <ControlTemplate TargetType="Button">
                                    <Border Width="18" Height="18"
                                        CornerRadius="9"
                                        Background="{TemplateBinding Background}">
                                        <ContentPresenter VerticalAlignment="Center"
                                                      HorizontalAlignment="Center"/>
                                    </Border>
                                </ControlTemplate>
                            </Button.Template>
                        </Button>

                    </StackPanel>

                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="67*"></ColumnDefinition>
                            <ColumnDefinition Width="195*"/>
                            <ColumnDefinition Width="240*"></ColumnDefinition>
                            <ColumnDefinition Width="22*"/>
                        </Grid.ColumnDefinitions>

                        <StackPanel Grid.Column="0"
                                    Orientation="Horizontal"
                                    VerticalAlignment="Center" Grid.ColumnSpan="2">

                            <TextBlock  Text="Welcome"
                                        Name="headerText"
                                       Foreground="Black"
                                       Margin="10"
                                       FontSize="20"
                                       FontFamily="Montserrat"
                                       FontWeight="DemiBold"
                                       VerticalAlignment="Center">
                            </TextBlock>
                        </StackPanel>
                    </Grid>

                    <!--<StackPanel Grid.Row="2">
                        <Image x:Name="fillImage" Stretch="Fill" Source="/photo_2023-02-25_17-53-59.jpg" >
                        </Image>
                    </StackPanel>-->
                    
                    
                    <!--Receptionist-->
                    <Border Grid.Row="2"
                            Background="#200F53"
                            CornerRadius="10"
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="receptionistGrid" 
                   >
                        <DataGrid Name="dataGrideReceptionist" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*"
                                  Background="Wheat"
                                  BorderThickness="0"
                                  GridLinesVisibility="None"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
                                  TextBlock.FontSize="12"
                                  
                                  >

                            <!--<DataGrid Name="dataGridCoach" CanUserAddRows="False" IsReadOnly="True" SelectionChanged="dataGrid_SelectionChangedCoach"/>-->
                            <DataGrid.Columns >


                                <DataGridTextColumn  HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Name" Binding="{Binding Name}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn  HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Address" Binding="{Binding Address}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn  HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Phone" Binding="{Binding Phone}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn  HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Salary" Binding="{Binding Salary}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn  HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Password" Binding="{Binding Password}" IsReadOnly="True" Width="*"/>

                                <!--Operations-->
                                <DataGridTemplateColumn  IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="3" Visibility="Visible" Name="Rupdate" >
                                                    <Canvas  Height="25" HorizontalAlignment="Left"  Name="U" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}"  Style="{StaticResource buttonIconHover}" Background="Transparent" BorderThickness="0" Click="RecEdit_Click">


                                                            <Icon:PackIconMaterial Kind="PencilOutline"
                                                                               
                                                                                Foreground="Wheat" 
                                                                                FontSize="8"
                                                                                Width="15"
                                                                                Height="15"
                                                                                   Margin="4,2">


                                                            </Icon:PackIconMaterial>
                                                        </Button>

                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="5" Visibility="Visible" Name="CDelete">
                                                    <Canvas  Height="25"  Name="D" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}" Background="Transparent" BorderThickness="0" Click="Rec_Delete_Click">

                                                            <Icon:PackIconMaterial Kind="DeleteOutline" 
                                                                             
                                                               Foreground="Wheat" 
                                                               FontSize="8"
                                                               Width="15"
                                                               Height="15"
                                                               Margin="4,2"
                                                               HorizontalAlignment="Left"
                                                               >
                                                            </Icon:PackIconMaterial>
                                                        </Button>
                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                            </DataGrid.Columns>

                        </DataGrid>
                    </Border>

                    <!-- Trainee grid-->
                    <Border Grid.Row="2"
                            Background="#200F53"
                            CornerRadius="10" 
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="traineeGrid"
                   >
                        <DataGrid Name="dataGrideTrainee" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*"
                                  Background="Wheat"
                                  BorderThickness="0"
                                  GridLinesVisibility="None"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
                                  TextBlock.FontSize="12"
                                  >

                            <DataGrid.Columns >
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Name" Binding="{Binding Name}" IsReadOnly="True" />
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Address" Binding="{Binding Address}" IsReadOnly="True" />
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Phone" Binding="{Binding Phone}" IsReadOnly="True"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Age" Binding="{Binding Age}" IsReadOnly="True"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Gender" Binding="{Binding Gender}" IsReadOnly="True"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Time" Binding="{Binding Time}" IsReadOnly="True"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Subscription" Binding="{Binding Subscription}" IsReadOnly="True"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Price" Binding="{Binding price}" IsReadOnly="True"/>


                                <!--Operations-->
                                <DataGridTemplateColumn  IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="3" Visibility="Visible" Name="Tupdate"  >
                                                    <Canvas  Height="25" HorizontalAlignment="Left"  Name="U" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}"  Style="{StaticResource buttonIconHover}" Background="Transparent" BorderThickness="0" Click="TrEdit_Click">


                                                            <Icon:PackIconMaterial Kind="PencilOutline"
                                                                               
                                                                                Foreground="Wheat" 
                                                                                FontSize="8"
                                                                                Width="15"
                                                                                Height="15"
                                                                                   Margin="4,2">


                                                            </Icon:PackIconMaterial>
                                                        </Button>

                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="5" Visibility="Visible" Name="CDelete">
                                                    <Canvas  Height="25"  Name="D" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}" Background="Transparent" BorderThickness="0" Click="TrDelete_Click">

                                                            <Icon:PackIconMaterial Kind="DeleteOutline" 
                                                                             
                                                               Foreground="Wheat" 
                                                               FontSize="8"
                                                               Width="15"
                                                               Height="15"
                                                               Margin="4,2"
                                                               HorizontalAlignment="Left"
                                                               >
                                                            </Icon:PackIconMaterial>
                                                        </Button>
                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                            </DataGrid.Columns>


                        </DataGrid>



                    </Border>

                    <!--Coach grid-->
                    <Border Grid.Row="2"
                            Background="#200F53"
                            CornerRadius="10"
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="coachGrid"
                   >
                        <DataGrid Name="dataGrideCoach" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*"
                                    Background="Wheat"
                                    BorderThickness="0"
                                  GridLinesVisibility="None"
                                  Margin="0,-3,0,3"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
                                  TextBlock.FontSize="12"
                                  >

                            <DataGrid.Columns >


                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Name" Binding="{Binding Name}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Address" Binding="{Binding Address}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Phone" Binding="{Binding Phone}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Salary" Binding="{Binding Salary}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Gender" Binding="{Binding Gender}" IsReadOnly="True" Width="*"/>
                                <DataGridTextColumn HeaderStyle="{StaticResource CenterGridHeaderStyle}" Header="Time" Binding="{Binding Time}" IsReadOnly="True" Width="*"/>

                                <!--Operations-->
                                <DataGridTemplateColumn  IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="3" Visibility="Visible" Name="Pupdate" >
                                                    <Canvas  Height="25" HorizontalAlignment="Left"  Name="U" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}"  Style="{StaticResource buttonIconHover}"  Background="Transparent" BorderThickness="0" Click="CEdit_Click" >


                                                            <Icon:PackIconMaterial Kind="PencilOutline"
                                                                               
                                                                                Foreground="Wheat" 
                                                                                FontSize="8"
                                                                                Width="15"
                                                                                Height="15"
                                                                                   Margin="4,2">


                                                            </Icon:PackIconMaterial>
                                                        </Button>

                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                                <DataGridTemplateColumn IsReadOnly="True" Width="0.25*">
                                    <DataGridTemplateColumn.CellTemplate>
                                        <DataTemplate>
                                            <StackPanel Orientation="Horizontal">

                                                <Border Grid.Row="5" Visibility="Visible" Name="PDelete">
                                                    <Canvas  Height="25"  Name="D" >
                                                        <Ellipse
                                                            Width="25"
                                                            Height="25"
                                                            Fill="#1C0E49"
                                                           />
                                                        <Button Template="{StaticResource NoMouseOverButtonTemplate}" Background="Transparent" BorderThickness="0" Click="CDelete_Click">

                                                            <Icon:PackIconMaterial Kind="DeleteOutline" 
                                                                             
                                                               Foreground="Wheat" 
                                                               FontSize="8"
                                                               Width="15"
                                                               Height="15"
                                                               Margin="4,2"
                                                               HorizontalAlignment="Left"
                                                               >
                                                            </Icon:PackIconMaterial>
                                                        </Button>
                                                    </Canvas>
                                                </Border>
                                            </StackPanel>
                                        </DataTemplate>
                                    </DataGridTemplateColumn.CellTemplate>
                                </DataGridTemplateColumn>

                            </DataGrid.Columns>


                            <!--<DataGrid.ColumnHeaderStyle>
                                <Style TargetType="DataGridColumnHeader">
                                    <Setter Property="Background" Value="#24105F"/>
                                    <Setter Property="Foreground" Value="#BCBEE0"/>
                                    <Setter Property="Padding" Value="10,0,0,10"/>
                                    <Setter Property="FontFamily" Value="Montserrat"/>
                                    <Setter Property="FontSize" Value="15"/>
                                </Style>
                            </DataGrid.ColumnHeaderStyle>-->


                        </DataGrid>
                    </Border>

                    <!--Protien product-->
                    <Border Grid.Row="2"
                            Background="#200F53"
                            CornerRadius="10"
                            Margin="50"
                            Visibility="Hidden"
                            Padding="10"
                            Name="protienGrid"
                   >
                        <DataGrid Name="dataGrideProtien" CanUserAddRows="False" ItemsSource="{Binding}" AutoGenerateColumns="False" ColumnWidth="*" 
                                   Background="Wheat"
                                   BorderThickness="0"
                                  GridLinesVisibility="None"
                                  TextBlock.TextAlignment="Center"
                                  AlternatingRowBackground="LightGray"
                                  TextBlock.FontWeight="Bold"
